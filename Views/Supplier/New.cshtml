@model Supplier

<div class="row">
<div class="col-lg-5">
  
  <div class="panel panel-primary">
      <div class="panel panel-heading">Sale Products From Daily Stocks to Customer</div>
      <div class="panel-body">
<form asp-action="New" asp-controller="Supplier" method="POST">
    <input type="hidden" asp-for="Id"/>

    <input type="hidden" value ="@ViewBag.Message" name="message" id="message"class="form-control" />
    
    <div class="form-group">
        <label asp-for="FirstName"></label>
        <input type="text" asp-for="FirstName" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="MiddleName"></label>
        <input type="text" asp-for="MiddleName" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="LastName"></label>
        <input type="text" asp-for="LastName" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input type="number" asp-for="PhoneNumber" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="MobileNumber"></label>
        <input type="number" asp-for="MobileNumber" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <input type="text" asp-for="Address" class="form-control"/>
    </div>

    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-info"/>
    </div>


</form>
      </div>
  </div>
</div>
</div>